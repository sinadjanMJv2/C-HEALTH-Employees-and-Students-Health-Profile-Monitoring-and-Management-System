<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script> *@


<script>
  $(document).ready(function () {





    $.ajax("../api/adminapi/getAllMedicine")
      .done(function (result) {




        //list of expired
        var formattedDate = new Date().toISOString().slice(0, 10);
        console.log("date now" + formattedDate);

        var expiremedicine = {};

        expiremedicine = result.filter(function (item) {
          return item.expiryDate <= formattedDate;
        });


        $(".notification").html(expiremedicine.length);
      });

    var retrievedArray = JSON.parse(sessionStorage.getItem('adminData'));

    if (retrievedArray == null) {

      console.log("login first")

    } else {

      retrievedArray.forEach(function (item) {
        console.log(item.gmail, item.password)
        $(".user-name").html(item.firstname + ' ' + item.lastname);




      });
    }



    let userType = window.sessionStorage.getItem("userType");

   
    if (userType == null) {

      console.log("login first")

    } else if(userType == "physician"){
     
        $(".user-role").html("Physician");

        $("#Medicine, #Personel, #Booked, #NormalRange, #Avail, #App ").css("display", "none");
        $("#Dept ,#Course").hide();


    }  else if(userType == "admin"){
    
        $(".user-role").html("Administrator");
       
    }



      $("#logout").click((e) => {

        logoutConfirmation();

      });

    function logoutConfirmation() {
      Swal.fire({
        title: 'Are you sure?',
        text: "You won't be logout this!",
        icon: 'warning',
        showCancelButton: true,
        confirmButtonColor: '#3085d6',
        cancelButtonColor: '#d33',
        confirmButtonText: 'Yes, Logout it!'
      }).then((result) => {
        if (result.isConfirmed) {

          Swal.fire(
            'Logout!',
            'You Have Logout your Account.',
            'success'
          )


          sessionStorage.clear();
          window.location.href = "/Page/SIGN";



        }
      })
    }

  });
</script>
