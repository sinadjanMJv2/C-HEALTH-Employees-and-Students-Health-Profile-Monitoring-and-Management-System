<div class="modal fade" id="modalAccountCreate" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
          <div class="modal-body d-flex justify-content-center">
        <h5 class="modal-title" id="exampleModalLabel">Create Admin Account</h5>
        </div>
        <button type="button" id="modalAccountCreateClose" class="btn-close btn-light" data-bs-dismiss="modal"
          aria-label="Close"></button>
      </div>

      <div class="modal-body AccountCreate d-flex justify-content-center">
        <div>

          <form id="createAdminAccountForm">
            <div class="form-group">
              <label name="" class="control-label">Full Name</label>
              <input type="text" name="Fullname" id="inputAdminAccountName" placeholder="Enter Admin name"
                class="form-control" required />
            </div>

            <div class="form-group">
              <label name="" class="control-label">Email</label>
              <input type="text" name="Email" id="inputAdminAccountEmail" placeholder="Enter Admin Email"
                class="form-control" required />
            </div>
            <div class="form-group">
              <label name="" class="control-label">Address</label>
              <input type="text" name="Address" id="inputAdminAccountAddress" placeholder="Enter Admin Address"
                class="form-control" required />
            </div>

            <div class="form-group">
              <label name="" class="control-label">Mobile Number</label>
              <input type="text" name="Mobilenumber" id="inputAdminAccountNum" placeholder="Enter Admin Mobile Number"
                class="form-control" required />
            </div>

            <div class="form-group">
              <label name="" class="control-label">Password</label>
              <input type="text" name="Password" id="inputAdminAccountPass" placeholder="Enter Admin Password"
                class="form-control" required />
            </div>

             </form>


         </div  >
      </div>
        <div class="modal-footer">
            <div class="modal-body d-flex justify-content-center">
              <button type="button" class="btn btn-primary" id="createAdminAccount">Confirm</button>
            </div>
        </div>

        </div>
      </div>
    </div>