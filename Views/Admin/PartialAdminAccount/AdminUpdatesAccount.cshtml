<div class="modal fade" id="modalAdminAccountUpdate" tabindex="-1" aria-labelledby="exampleModalLabel"
  aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
          <div class="modal-body d-flex justify-content-center">
        <h5 class="modal-title" id="exampleModalLabel">Update Admin Account</h5>
       </div>
        <button type="button" id="modalAdminAccountUpdateClose" class="btn-close btn-light" data-bs-dismiss="modal"
          aria-label="Close"></button>
      </div>

      <div class="modal-body adminAcntUpdate d-flex justify-content-center">
        <div>

          <form id="updateAdminAccountForm">
            <div class="form-group">
              <label name="" class="control-label">Full Name</label>
              <input type="text" name="Fullname" id="inputAdminAccountName" class="form-control" required />
            </div>

            <div class="form-group">
              <label name="" class="control-label">Email</label>
              <input type="text" name="Email" id="inputAdminAccountEmail" class="form-control" required />
            </div>
            <div class="form-group">
              <label name="" class="control-label">Address</label>
              <input type="text" name="Address" id="inputAdminAccountAddress" class="form-control" required />
            </div>

            <div class="form-group">
              <label name="" class="control-label">Mobile Number</label>
              <input type="text" name="Mobilenumber" id="inputAdminAccountNum" class="form-control" required />
            </div>

            <div class="form-group">
              <label name="" class="control-label">Password</label>
              <input type="text" name="Password" id="inputAdminAccountPass" class="form-control" required />
            </div>

          </form>


        </div>
      </div>

      <div class="modal-footer">
        <div class="modal-body d-flex justify-content-center">
          <button type="button" class="btn btn-primary" id="updateADMINacc">Confirm</button>
        </div>
      </div>


    </div>
  </div>
</div>
</div>