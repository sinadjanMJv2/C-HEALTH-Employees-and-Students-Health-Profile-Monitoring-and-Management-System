<div class="modal fade" id="modalDoctorCreate" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
          <div class="modal-body d-flex justify-content-center">
        <h5 class="modal-title" id="exampleModalLabel">Create Doctor</h5>
        </div>
        <button type="button" id="modalDoctorCreateClose" class="btn-close btn-light" data-bs-dismiss="modal"
          aria-label="Close"></button>
      </div>
      <div class="modal-body DoctorCreate d-flex justify-content-center">
        <div>
          <div>


            <form id="createDoctorForm">

              <div class="form-group">
                <label name="" class="control-label">Doctor First Name</label>
                <input type="text" name="DoctorFirstName" id="inputDoctorFirstName" placeholder="Enter Doctor First Name"
                  class="form-control" required />
              </div>

               <div class="form-group">
                <label name="" class="control-label">Doctor Last Name</label>
                <input type="text" name="DoctorLastName" id="inputDoctorLastName" placeholder="Enter Doctor Last Name"
                  class="form-control" required />
              </div>

               <div class="form-group">
                <label name="" class="control-label">Doctor Address</label>
                <input type="text" name="DoctorAddress" id="inputDoctorAddress" placeholder="Enter Doctor Address"
                  class="form-control" required />
              </div>

               <div class="form-group">
                <label name="" class="control-label">Doctor G-mail Account</label>
                <input type="text" name="DoctorGmailAcc" id="inputDoctorGmailAcc" placeholder="Enter Doctor G-mail Account"
                  class="form-control" required />
              </div>

              <div class="form-group">
                <label name="Status" class="control-label">Status</label>
                <br>
                <center>
                  <input name="Status" class="form-check-input stat" type="checkbox" value="Active"
                    id="flexCheckChecked" />
                  <label class="form-check-label" for="flexCheckChecked">Active</label>
                </center>
              </div>

             

            </form>

          </div>
        </div>
      </div>
      <div class="modal-footer">
        <div class="modal-body d-flex justify-content-center">
          <button type="button" class="btn btn-primary" id="createDoctor">Confirm</button>
        </div>
      </div>


    </div>
  </div>
</div>