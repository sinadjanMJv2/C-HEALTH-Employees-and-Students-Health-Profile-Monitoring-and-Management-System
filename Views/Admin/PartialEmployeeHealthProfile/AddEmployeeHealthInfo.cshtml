<script>
   // Sample data
    var physiciansData = [];
    
            $.ajax("../api/adminapi/getPhysician")
                .done(function (result) {
                    physiciansData = result;
                });

    // Function to populate the select element
    function populatePhysiciansSelect(data) {
        var selectElement = $('#physicians');

        // Clear existing options
        selectElement.empty();

        // Iterate over the data and append options
        $.each(data, function(index, physician) {
            selectElement.append($('<option>', {
                value: physician.fullname,
                text: physician.fullname.trim() // Trim to remove leading/trailing spaces
            }));
        });
    }

    // Document ready function
    $(document).ready(function() {
        // Call the function to populate the select element
        populatePhysiciansSelect(physiciansData);
    });
</script>
