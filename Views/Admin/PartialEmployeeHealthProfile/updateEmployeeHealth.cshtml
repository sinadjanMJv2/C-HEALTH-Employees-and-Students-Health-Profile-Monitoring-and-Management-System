                                                <label name="" class="control-label">Hemoglobin</label>
                                                <input type="number" name="Hemoglobin" id="inputMedicineName"
                                                    class="form-control" required />
                                            </div>
                                        </div>
                                        <div class="col-sm-3">
                                            <div class="form-group">
                                                <label name="" class="control-label">Units</label>
                                                <select id="hemoglobin"  name="Hemoglobinunits" class="form-select"></select>
                                            </div>
                                        </div>



                                        <div class="col-sm-3">
                                            <div class="form-group">
                                                <label name="" class="control-label">Hematocrit</label>
                                                <input type="number" name="Hematocrit" id="inputMedicineName"
                                                    class="form-control" required />
                                            </div>
                                        </div>
                                        <div class="col-sm-3">
                                            <div class="form-group">
                                                <label name="" class="control-label">Units</label>
                                                <select id="hematocrit"  name="Hematocritunits" class="form-select"></select>
                                            </div>
                                        </div>



                                        <div class="col-sm-3">
                                            <div class="form-group">
                                                <label name="" class="control-label">RBC</label>
                                                <input type="number" name="Rbc" id="inputMedicineName"
                                                    class="form-control" required />
                                            </div>
                                        </div>
                                        <div class="col-sm-3">
                                            <div class="form-group">
                                                <label name="" class="control-label">Units</label>
                                                <select id="rbc"  name="Rbcunits" class="form-select"></select>
                                            </div>
                                        </div>



                                        <div class="col-sm-3">
