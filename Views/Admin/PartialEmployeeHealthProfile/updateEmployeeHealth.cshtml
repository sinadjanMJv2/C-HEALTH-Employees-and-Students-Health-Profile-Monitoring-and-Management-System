<body id="body">




    <div class="modal fade" id="modalEmployeeHealthUpdate" tabindex="-1" aria-labelledby="exampleModalLabel"
        aria-hidden="true">
        <div class="modal-dialog ">
            <div class="modal-content" id="contentvol">
                <div class="modal-header">

                    <button type="button" id="modalEmployeeAddClose" class="btn-close btn-light" data-bs-dismiss="modal"
                        aria-label="Close"></button>
                </div>

                <main id="main">
                    <article>



                        <template id="unitsOptionTemplate">
                            <option value=""></option>
                        </template>
                         <template id="phyOptionTemplate">
                            <option value=""></option>
                        </template>

                        <!-- Start Multiform HTML -->
                        <section class="multi_step_form">
                            
                            <form id="EmployeeHealthUpdateForm">

                                <!-- Tittle -->
                                <div class="tittle">
                                    <h2>Health Information Process</h2>
                                    <p>In order to use this service, you have to complete this process</p>
                                </div>
                                <!-- progressbar -->
                                <ul id="progressbar">
                                    <li class="active">Commplete Blood Count</li>
                                    <li>Urinalysis</li>
                                    <li>Security Questions</li>
                                </ul>
                                <!-- fieldsets -->
                                <fieldset>
                                    <h3 style="text-align: center; ">Input Credentials</h3>
                                    @* <h6>We will send you a SMS. Input the code to verify.</h6> *@
                                      <hr style="margin:25px 0px 25px;" />
                                    <div class="row">

                                          <div class="col-sm-12">
                                            <div class="form-group">
                                                <span class="lebel">Physician</span>

                                                <input type="text" name="Cbcphysician" id="hn"
                                                     class="form-control" required />
                                            </div>
                                        </div>

                                 
                                        
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <span class="lebel">Hospital No.</span>

                                                <input type="text" name="Hospitalnumber" id="hn"
                                                    placeholder="Enter Hospital No." class="form-control" required />
                                            </div>
                                        </div>



                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label name="" class="control-label"> Date Time Requested</label>
                                                <input type="datetime-local" name="Cbcdatetimerequested" id="dtr"
                                                    class="form-control" required />
                                            </div>
                                        </div>


                                        <div class="col-sm-6">
                                            <div class="form-group">
                                                <label>Draw Date Time</label>
                                                <input type="datetime-local" name="Cbcdrawdatetime" id="ddt"
                                                    class="form-control" required />
                                            </div>
                                        </div>
                                        <hr style="margin:25px 0px 50px;" />



                                        <div class="col-sm-3">
                                            <div class="form-group">
                                                <label class="control-label">WBC</label>
                                                <input type="number" name="Wbc" id="inputMedicineName"
                                                    class="form-control" required />
                                            </div>
                                        </div>

                                         <div class="col-sm-3">
                                            <div class="form-group">
                                                <label name="" class="control-label">Units</label>
                                                <select id="wbc" name="Wbcunits" class="form-select"></select>
                                            </div>
                                        </div>
                                       




                                        <div class="col-sm-3">
                                            <div class="form-group">
                                                <label name="" class="control-label">Neutrophils</label>
                                                <input   type="number" name="Neutrophils" id="inputMedicineName"
                                                    class="form-control" required />
                                            </div>
                                        </div>
                                        <div class="col-sm-3">
                                            <div class="form-group">
                                                <label name="" class="control-label">Units</label>
                                                <select id="neutrophils" name="Neutrophilsunits" class="form-select"></select>
                                            </div>
                                        </div>




                                        <div class="col-sm-3">
                                            <div class="form-group">
                                                <label name="" class="control-label">Lymphocyte</label>
